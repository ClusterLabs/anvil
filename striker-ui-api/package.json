{
  "name": "striker-ui-api",
  "version": "0.1.0",
  "description": "API for striker-ui",
  "scripts": {
    "build": "pkg --compress GZip --output striker-ui-api --targets node14-linux index.js",
    "clean": "rm -f striker-ui-api",
    "dev": "node index.js",
    "rebuild": "npm run clean && npm run build",
    "start": "npm run build && ./striker-ui-api"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "pkg": "^5.5.1",
    "prettier": "^2.5.0"
  }
}
